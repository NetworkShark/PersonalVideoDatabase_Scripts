procedure BypassAgeConfirmation();
var
	URL, outputFilePath, CredentialPath: String;
begin
	LogMessage('Called BypassAgeConfirmation');
	CredentialPath := GetAppPath() + BASE_DOWNLOAD + 'AdultDVDEmpire_cred.txt';
	URL := 'https://www.adultempire.com/Account/AgeConfirmation?ageConfirmationClicked=true';
